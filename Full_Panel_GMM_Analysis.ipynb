    ,
    "\n",
    "import pandas as pd\n",
    "import numpy as np\n",
    "import statsmodels.api as sm\n",
    "import matplotlib.pyplot as plt\n",
    "import seaborn as sns\n",
    "from linearmodels.panel import PanelOLS, RandomEffects, FirstDifferenceGMM\n",
    "from statsmodels.stats.outliers_influence import variance_inflation_factor\n",
    "from linearmodels.iv import IVGMM\n",
    "\n",
    "# Load dataset\n",
    "csv_path = '/mnt/data/final_data_21countries.csv'\n",
    "df = pd.read_csv(csv_path)\n",
    "df.columns = df.columns.str.lower()\n",
    "\n",
    "# Create new variables\n",
    "df['ict_trade'] = ((df['ictex'] * df['ex'] * 10) + (df['ictim'] * df['im'] * 10)) * 100 / ((df['ex'] + df['im']) * 1000)\n",
    "df['ln_internet'] = np.log((df['internet'] / 100) * df['pop'])\n",
    "df['ln_mcs'] = np.log(df['pmcs'])\n",
    "\n",
    "# OLS regression\n",
    "X = df[['ln_mcs', 'tel', 'ln_internet', 'ict_trade', 'gtech', 'grpop', 'dgdp', 'unem', 'grural']]\n",
    "X = sm.add_constant(X)\n",
    "y = df['grgdp']\n",
    "ols_model = sm.OLS(y, X).fit()\n",
    "print(ols_model.summary())\n",
    "\n",
    "# VIF check\n",
    "vif_data = pd.DataFrame()\n",
    "vif_data['feature'] = X.columns\n",
    "vif_data['VIF'] = [variance_inflation_factor(X.values, i) for i in range(X.shape[1])]\n",
    "print(vif_data)\n",
    "\n",
    "# Correlation matrix\n",
    "corr_matrix = df[['grgdp', 'ln_mcs', 'tel', 'ln_internet', 'ict_trade', 'gtech', 'grpop', 'dgdp', 'unem', 'grural']].corr()\n",
    "plt.figure(figsize=(10,8))\n",
    "sns.heatmap(corr_matrix, annot=True, cmap='coolwarm')\n",
    "plt.title('Correlation Matrix')\n",
    "plt.show()\n",
    "\n",
    "# Set panel data index\n",
    "df = df.set_index(['id', 'year'])\n",
    "\n",
    "# Pooled OLS\n",
    "pooled_model = PanelOLS.from_formula(\n",
    "    'grgdp ~ ln_mcs + tel + ln_internet + ict_trade + gtech + grpop + dgdp + unem + grural + EntityEffects', data=df).fit()\n",
    "print(pooled_model.summary)\n",
    "\n",
    "# Random Effects Model\n",
    "re_model = RandomEffects.from_formula(\n",
    "    'grgdp ~ ln_mcs + tel + ln_internet + ict_trade + gtech + grpop + dgdp + unem + grural', data=df).fit()\n",
    "print(re_model.summary)\n",
    "\n",
    "# Fixed Effects Model\n",
    "fe_model = PanelOLS.from_formula(\n",
    "    'grgdp ~ ln_mcs + tel + ln_internet + ict_trade + gtech + grpop + dgdp + unem + grural + EntityEffects', data=df).fit()\n",
    "print(fe_model.summary)\n",
    "\n",
    "# Manual Hausman Test\n",
    "b = fe_model.params\n",
    "B = re_model.params\n",
    "var_b = fe_model.cov\n",
    "var_B = re_model.cov\n",
    "hausman_stat = (b - B).T @ np.linalg.inv(var_b - var_B) @ (b - B)\n",
    "print(\"Hausman Test Chi-Square Statistic:\", hausman_stat)\n",
    "\n",
    "# First Difference GMM\n",
    "fd_gmm_model = FirstDifferenceGMM.from_formula(\n",
    "    'grgdp ~ 1 + [ln_mcs + tel + ln_internet + ict_trade + gtech + grpop + dgdp + unem + grural ~ lag(grgdp, 1)]',\n",
    "    data=df\n",
    ").fit()\n",
    "print(fd_gmm_model.summary)\n",
    "\n",
    "# System GMM manually (using lags and external instruments)\n",
    "df['l1_grgdp'] = df.groupby('id')['grgdp'].shift(1)\n",
    "df['l2_ict_trade'] = df.groupby('id')['ict_trade'].shift(2)\n",
    "df['l1_ln_internet'] = df.groupby('id')['ln_internet'].shift(1)\n",
    "df['l3_dgdp'] = df.groupby('id')['dgdp'].shift(3)\n",
    "df['l2_unem'] = df.groupby('id')['unem'].shift(2)\n",
    "df['l1_ln_mcs'] = df.groupby('id')['ln_mcs'].shift(1)\n",
    "df['l1_tel'] = df.groupby('id')['tel'].shift(1)\n",
    "df['l5_grural'] = df.groupby('id')['grural'].shift(5)\n",
    "\n",
    "df_sys = df.dropna(subset=['l1_grgdp', 'l2_ict_trade', 'l1_ln_internet', 'l3_dgdp', 'l2_unem', 'l1_ln_mcs', 'l1_tel', 'l5_grural'])\n",
    "endog = df_sys[['grgdp']]\n",
    "exog = sm.add_constant(df_sys[['ln_mcs', 'tel', 'ln_internet', 'ict_trade', 'gtech', 'grpop', 'dgdp', 'unem', 'grural']])\n",
    "instr = sm.add_constant(df_sys[['l1_grgdp', 'l2_ict_trade', 'l1_ln_internet', 'l3_dgdp', 'l2_unem', 'l1_ln_mcs', 'l1_tel', 'gtech', 'grpop', 'l5_grural']])\n",
    "\n",
    "sys_gmm_model = IVGMM(endog=endog, exog=exog, instrument=instr)\n",
    "sys_gmm_res = sys_gmm_model.fit()\n",
    "print(sys_gmm_res)\n"
   ]
  }
